[hub]
        protocol = https
[user]
	name = Ron Hong
	email = ron@datadoghq.com
[credential]
	helper = osxkeychain
[alias]
[color]
        diff = auto
        status = auto
        branch = auto
        ui = true
[core]
        excludesfile = ~/.gitignore
	editor = vim
[apply]
        whitespace = nowarn
[help]
        autocorrect = 1
[status]
	short = true
	branch = true
[pull]
	rebase = true
[log]
	abbrevCommit = true
	decorate = true
[format]
	pretty = oneline

